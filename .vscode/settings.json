{
	"typescript.tsdk": "node_modules/typescript/lib",
    "files.exclude": {
		"node_modules": true,
		"bower_components": true,
		"typings": true,
		"app/**/*.js": true,
		"app/**/*.js.map": true,
		"app/**/*.css": true,
		"obj": true,
		"bin": true,
		"build": true,
		"omnisharp": true,
		"omnisharp-win-x64-netcoreapp1*": true,
		"omnisharp-ubuntu-x64-netcoreapp1*": true
	},
	"search.exclude": {
		"ream-editor-win32-x64": true,
		"ream-editor-linux-x64": true
	}
}